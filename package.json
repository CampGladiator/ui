  "devDependencies": {
    "@babel/core": "^7.3.4",
    "@storybook/addon-a11y": "^5.0.5",
    "@storybook/addon-actions": "^5.0.3",
    "@storybook/addon-centered": "^5.0.3",
    "@storybook/addon-info": "^5.0.5",
    "@storybook/addon-links": "^5.0.3",
    "@storybook/addon-viewport": "^5.0.3",
    "@storybook/addons": "^5.0.3",
    "@storybook/react": "^5.0.3",
    "@storybook/storybook-deployer": "^2.8.1",
    "autoprefixer": "^9.4.7",
    "babel-loader": "^8.0.5",
    "clean-css-cli": "^4.2.1",
    "concurrently": "^4.1.0",
    "node-sass": "^4.11.0",
    "postcss-cli": "^6.1.1",
    "prettier": "^1.16.4",
    "react-storybook-decorator-background": "^1.0.5",
    "rimraf": "^2.6.3",
    "stylelint": "^9.10.1",
    "stylelint-config-recommended": "^2.1.0",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-scss": "^3.5.4"
  },
  "scripts": {
    "build": "npm run build:clean && npm run build:sass && npm run build:autoprefix && npm run build:cleancss",
    "build:autoprefix": "postcss --use autoprefixer --map false --output build/main.css build/main.css",
    "build:cleancss": "cleancss -o build/main.min.css build/main.css",
    "build:clean": "rimraf css",
    "build:sass": "node-sass --error-bell --output-style expanded --source-map true main.sass build/main.css",
    "build:watch": "yarn run build:sass --watch",
    "start": "yarn run build:sass && concurrently \"yarn run storybook\" \"yarn run build:watch\"",
    "lint": "stylelint sass/**/*.sass --syntax sass",
    "lint:fix": "yarn run lint --fix",
    "storybook": "start-storybook -p 6006",
    "storybook:build": "build-storybook",
    "deploy": "storybook-to-ghpages"
  },
  "prettier": {
    "singleQuote": true,
    "semi": false,
    "trailingComma": "all"
  },
  "homepage": "https://github.com/CampGladiator/CGUI#readme",
  "dependencies": {
    "normalize.css": "^8.0.1"
  }
}
